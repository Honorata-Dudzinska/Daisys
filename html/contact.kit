<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Daisy's</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Montserrat:wght@200;300;400;700&family=Noto+Serif+Display&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="dist/css/main.min.css">
</head>

<body>

    <!--@include 'nav.kit'-->

    <!--@include 'header.kit'-->


    <section class="contact wrapper" id="contact">
        <div class="contact__info-container">
            <div class="contact__info contact__info--location">
                <h4>Adres:</h4>
                <img src="./dist/img/svg/loccontact.svg" alt="">
                <p>Rzeszów</p>
            </div>

            <div class="contact__info contact__info--instagram">
                <h4>Instagram:</h4>
                <a class="contact-links" href="instagram.com"><img src="./dist/img/svg/igcontact.svg" alt=""></a>
                <br>
                <a class="contact-links" href="instagram.com">@Daisy's</a>
            </div>

            <div class="contact__info contact__info--facebook">
                <h4>Facebook:</h4>
                <a class="contact-links" href="facebook.com"><img src="./dist/img/svg/fbcontact.svg" alt=""></a>
                <br>
                <a class="contact-links" href="facebook.com">@Daisy's</a>
            </div>

            <div class="contact__info contact__info--telephone">
                <h4>Telefon:</h4>
                <a class="contact-links" href="tel:+48 555 444 333"><img src="./dist/img/svg/telcontact.svg" alt=""></a>
                <br>
                <a class="contact-links" href="tel:+48 555 444 333">+48 555 444 333</a>
            </div>
        </div>

        <div class="contact__map">
            <h2>Znajdź nas :</h2>
            <div class="google-map"><iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2562.3069433271257!2d21.99600797684228!3d50.04308111661123!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x473cfbdfbd6dde71%3A0xe27378e26741bc4d!2sRzesz%C3%B3w%20Centrum!5e0!3m2!1spl!2spl!4v1695387709135!5m2!1spl!2spl" width="300" height="300" style="border: solid 1px;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe></div>
        </div>

    </section>


    <!--@include 'footer.kit'-->

    <script src="dist/js/main.min.js"></script>
</body>

</html>